{
    "minimum-stability" : "RC",
    "name" : "luyadev/luya-module-mailchimp",
    "description" : "This module provides a simple way to build your own form for a user registration in a MailChimp newsletter.",
    "type" : "luya-module",
    "keywords" : ["php", "luya", "module", "cms", "yii2", "yii", "mailchimp", "form", "registration", "newsletter", "luya-module"],
    "license": "MIT",
    "homepage" : "https://luya.io",
    "authors" : [
        {
          "name" : "Martin Petrasch",
          "email" : "git@martinpetrasch.com",
          "homepage" : "https://github.com/martinpetrasch"
        },
        {
            "name" : "Basil Suter",
            "email" : "git@nadar.io",
            "homepage" : "https://github.com/nadar"
        }
    ],
    "support" : {
        "issues" : "https://github.com/luyadev/luya/issues"
    },
    "require" : {
        "mailchimp/mailchimp": "^2.0"
    },
    "require-dev" : {
        "luyadev/luya-core" : "1.0.0-RC4",
        "luyadev/luya-testsuite":"~1.0.0"
    },
    "autoload" : {
        "psr-4" : {
            "luya\\mailchimp\\" : "src/",
            "luya\\mailchimp\\tests\\" : "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}